{
  "icon_theme": "Colored Zed Icons Theme Dark",
  "context_servers": {
    "nx-mcp": {
      "env": {},
      "args": [
        "dlx",
        "nx-mcp@latest"
      ],
      "command": "pnpm",
      "source": "custom",
      "enabled": true
    }
  },
  "base_keymap": "JetBrains",
  "edit_predictions": {
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent": {
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4"
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "theme": "Github Dark",
  "ui_font_family": "Monaspace Neon",
  "buffer_font_family": "Monaspace Neon",
  "vim_mode": false,
  "vim": {
    "use_system_clipboard": "always"
  },
  "cursor_blink": false,
  "relative_line_numbers": true,
  "scroll_beyond_last_line": "off",
  "vertical_scroll_margin": 8,
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false
  },
  "tabs": {
    "show_diagnostics": "errors",
    "file_icons": true,
    "git_status": true
  },
  "scrollbar": {
    "show": "never"
  },
  "diagnostics": {
    "include_warnings": true,
    "inline": {
      "enabled": true,
      "update_debounce_ms": 150,
      "padding": 4,
      "min_column": 0,
      "max_severity": null
    }
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware"
  },
  "autosave": "on_focus_change",
  "soft_wrap": "editor_width",
  "search": {
    "whole_word": false,
    "case_sensitive": true,
    "include_ignored": false,
    "regex": false
  },
  "file_finder": {
    "modal_max_width": "medium"
  },
  "file_types": {
    "Dockerfile": ["Dockerfile", "Dockerfile.*"],
    "JSON": ["json", "jsonc", "*.code-snippets"],
    "Bazel": ["BUILD", "BUILD.bazel", "MODULE", "MODULE.bazel", "*.bzl"]
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    // above is default from Zed
    "**/out",
    "**/dist",
    "**/.husky",
    "**/.turbo",
    "**/.vscode-test",
    "**/.vscode",
    "**/.idea",
    "**/.next",
    "**/.storybook",
    "**/.tap",
    "**/.nyc_output",
    "**/report",
    "**/node_modules",
    "**/.zed",
    "**/.yarn",
    "**/.nx",
    "**/.jj",
    "**/.gemini"
  ],
  "file_scan_inclusions": [".env"],
  "terminal": {
    "env": {
      "Editor": "zed --wait"
    },
    "detect_venv": {
      "on": {
        "directories": [".venv", "venv"],
        "activate_script": "default"
      }
    },
    "button": false
  },
  "toolbar": {
    "quick_actions": false
  },
  "project_panel": {
    "default_width": 300,
    "dock": "left",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "scrollbar": {
      "show": "never"
    }
  },
  "outline_panel": {
    "dock": "right",
    "button": false
  },
  // "notification_panel": {
  //   "button": false
  // },
  "collaboration_panel": {
    "dock": "left",
    "button": false
  },
  "git_panel": {
    "dock": "left",
    "button": false
  },
  "chat_panel": {
    "dock": "right"
  },
  "languages": {
    "Rust": {
      "formatter": "language_server",
      "format_on_save": "language_server"
    },
    "Go": {
      "formatter": "language_server",
      "format_on_save": "language_server"
    }
  }
}
